@page
@model TNAUPMS.Pages.SoilTest
@{
    ViewData["Title"] = "Application for Soil test result Interpretation";
}

@{ Layout = "Shared/_Layoutadmin.cshtml"; }
<style>
.row{
    margin-bottom:10px;
}
.tbl{
    width:100%;
}
.tdlabel{
    width : 60%;
}
.tdresult{
    width : 30%;
}
.tdunit{
    width : 10%;
}

</style>
<div class="header-body">
    <div class="header-body-left">
        <h3 class="page-title">Application for Soil test result Interpretation</h3>
    </div>
</div>

<div class="row">
    <div class="col-md-2">
       <label>Farmer Name</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The Farmer Name is required." class="form-control" id="FarmerName" name="FarmerName">
    </div>
    <div class="col-md-2">
       <label>Village</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The Village Name is required." class="form-control" id="Village" name="Village">
    </div>
</div>

<div class="row">
    <div class="col-md-2">
       <label>Taluk Name</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The Taluk Name is required." class="form-control" id="Taluk" name="Taluk">
    </div>
    <div class="col-md-2">
       <label>District</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The District Name is required." class="form-control" id="District" name="District">
    </div>
</div>
<div class="row">
    <div class="col-md-2">
       <label>Mobile No</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The Mobile No is required." class="form-control" id="Mobile" name="Mobile">
    </div>
    <div class="col-md-2">
       <label>Farm survey no</label>
    </div>
    <div class="col-md-4">
       <input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="surveyno" name="surveyno">
    </div>
</div>
<div class="row">
    <div class="col-md-12" style="background-color:darkgreen;height:10px;">
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">PH:</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>  </td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">EC:</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>dS/m</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Organic carbon(OC):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>g/kg</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Nitrogen(N):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>kg/ha</td>
            </tr>
        </table>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Phosphorous(P):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>kg/ha</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Potassium(K):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>kg/ha</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Iron(Fe):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>ppm</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Manganese(Mn):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>ppm</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Copper(Cu):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>ppm</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Zinc(Zn):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>ppm</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Boron(B):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>ppm</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Cation Exchange capacity(CEC):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>cm/kg </td>
            </tr>
        </table>
    </div>
</div>


<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Exchangeable sodium percentage(ESP):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td></td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Sodium Adsorption ration(SAR):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                 <td></td>
            </tr>
        </table>
    </div>
</div>


<div class="row">
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Calcium carbonate content(CaCO3):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ph" name="ph"></td>
                <td>%</td>
            </tr>
        </table>
    </div>
    <div class="col-md-6">
        <table class="tbl">
            <tr>
                <td class="tdlabel">Gypsum content(CaSO4):</td>
                <td class="tdresult"><input type="text" data-val="true" data-val-required="The Farm survey is required." class="form-control" id="ec" name="ec"></td>
                <td>%</td>
            </tr>
        </table>
    </div>
</div>

<div class="row">
    <div class="col-md-12" style="text-align:center;">
        <button class="btn btn-sm btn-success">Get Result</button>
    </div>
</div>